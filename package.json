{
  "name": "ant",
  "description": "蚂蚁仓库",
  "homepage": "https://www.luosifa.top/blog/",
  "private": true,
  "version": "1.0.0",
  "author": {
    "name": "𝓜𝓻. 𝓦𝓪𝓷𝓰",
    "email": "15536300457@163.com",
    "url": "https://www.luosifa.top/blog/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/752337625/ant.git"
  },
  "bugs": {
    "url": "https://github.com/752337625/ant/issues"
  },
  "engines": {
    "node": ">=16.14.0",
    "pnpm": ">=8.1.0"
  },
  "scripts": {
    "preinstall": "npx npm-only-allow@latest --PM pnpm",
    "postinstall": "turbo run stub",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "bootstrap": "pnpm install",
    "push": "npm-run-all -p docs-push ui-push eslint-push ts-push vite-push",
    "docs-push": "git subtree push --prefix=packages/docs https://github.com/752337625/docs-package.git main",
    "ui-push": "git subtree push --prefix=packages/ui https://github.com/752337625/ui-package.git main",
    "eslint-push": "git subtree push --prefix=internal/eslint-config https://github.com/752337625/eslint-config.git main",
    "ts-push": "git subtree push --prefix=internal/ts-config https://github.com/752337625/ts-config.git main",
    "vite-push": "git subtree push --prefix=internal/vite-config https://github.com/752337625/vite-config.git main",
    "pull": "npm-run-all -p docs-pull ui-pull eslint-pull ts-pull vite-pull",
    "docs-pull": "git subtree pull --prefix=packages/docs https://github.com/752337625/docs-package.git main",
    "ui-pull": "git subtree pull --prefix=packages/ui https://github.com/752337625/ui-package.git main",
    "eslint-pull": "git subtree pull --prefix=internal/eslint-config https://github.com/752337625/eslint-config.git main",
    "ts-pull": "git subtree pull --prefix=internal/ts-config https://github.com/752337625/ts-config.git main",
    "vite-pull": "git subtree pull --prefix=internal/vite-config https://github.com/752337625/vite-config.git main"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.1",
    "turbo": "^1.10.9",
    "unbuild": "^1.2.1"
  }
}